            nv_data.addColumn('number', '자사몰 매출 비율');


            // console.log(d_list[2])
            nv_data.addRows(d_list[3])

            var nv_table = new google.visualization.Table(document.getElementById("nv_table"));
            nv_table.draw(nv_data, { showRowNumber: false, width: '100%', height: '100%', backgroundColor: { fill: 'transparent' } });


        }

        var xhr = null;

        function getXmlHttpRequestObject() {
            if (!xhr) {
                xhr = new XMLHttpRequest();
            }
            return xhr;
        };

        function dataCallback() {
            if (xhr.readyState == 4 && xhr.status == 200) {
                tables(JSON.parse(xhr.responseText));

            }
        }

        function summary() {
            // xhr = getXmlHttpRequestObject();
            xhr = new XMLHttpRequest();
            xhr.onreadystatechange = dataCallback;
            // console.log(day)
          
