            fb_data.addColumn('number', '지출 금액'); //spend
            fb_data.addColumn('number', '영업 이익');


            fb_data.addRows(d_list[0])

            var fb_table = new google.visualization.Table(document.getElementById("fb_table"));
            fb_table.draw(fb_data, { showRowNumber: false, width: '100%', height: '100%', backgroundColor: { fill: 'transparent' } });

            //  asdfaaasdfasdfasdfasdfasdfasfdsfsa

