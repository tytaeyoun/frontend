            po_data.addColumn('string', '최대 판매처'); //ctr


            po_data.addRows(d_list[1])

            var po_table = new google.visualization.Table(document.getElementById("po_table"));
            po_table.draw(po_data, { showRowNumber: false, width: '100%', height: '100%', backgroundColor: { fill: 'transparent' } });



            //  asdfaaasdfasdfasdfasdfasdfasfdsfsa

            var tf_data = new google.visualization.DataTable();
            tf_data.addColumn('string', '키워드');
            tf_data.addColumn('number', '상품수');
            tf_data.addColumn('number', '평균 구좌수');
